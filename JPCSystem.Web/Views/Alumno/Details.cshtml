@model JPCSystem.Domain.Alumno

@{
    ViewBag.Title = "Details";
    Layout = null;
}


<br/>
<div>
    <h2>
        Alumno: 
        @Html.DisplayFor(model => model.ApPaterno)
        @Html.DisplayFor(model => model.ApMaterno),
        @Html.DisplayFor(model => model.Nombre)

    </h2>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Documento: 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NumeroDocumento)
        </dd>

        <dt>
            Departamento: 
        </dt>

        <dd>
            @ViewBag.deparmeto
        </dd>

        <dt>
            Distrito: 
        </dt>

        <dd>
            @ViewBag.distrito
        </dd>

        <dt>
            Direccion
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Direccion)
        </dd>

        <dt>
            Preovincia
        </dt>

        <dd>
            @ViewBag.provincia
        </dd>

        <dt>
            Fecha de Nacimiento: 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaNacimiento)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Correo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Correo)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Genero): 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Genero)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Telefono): 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Telefono)
        </dd>

    </dl>
</div>
